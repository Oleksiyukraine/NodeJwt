{
  "name": "hosting",
  "version": "0.1.0",
  "description": "Hosting",
  "author": "Jocus",
  "main": "src/index.js",
  "private": false,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=7.6",
    "yarn": "*"
